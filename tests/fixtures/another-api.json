{
  "openapi": "3.0.0",
  "info": {
    "title": "Another API",
    "version": "2.0.0",
    "description": "Another sample API for testing"
  },
  "paths": {
    "/products": {
      "get": {
        "summary": "Get all products",
        "description": "Retrieve a list of all products",
        "operationId": "getProducts",
        "responses": {
          "200": {
            "description": "Successful response"
          }
        }
      }
    },
    "/products/{id}": {
      "delete": {
        "summary": "Delete product",
        "description": "Delete a specific product",
        "operationId": "deleteProduct",
        "responses": {
          "204": {
            "description": "Product deleted"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Product": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "price": {
            "type": "number"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "username": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "apiKey": {
        "type": "apiKey",
        "in": "header",
        "name": "X-API-Key"
      }
    }
  }
}